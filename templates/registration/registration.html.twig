{% extends 'base.html.twig' %}

{% block title %}Enregistrement{% endblock %}

{% block body %}

	<h1>Enregistrement</h1>

	{% for error in errors %}
		<div class="error">{{ error }}</div>
	{% endfor %}

	{{ form_start(form) }}
	{{ form_widget(form) }}
	<button type="submit">{{ button_label|default("S'enregistrer") }}</button>
	{{ form_end(form) }}

{% endblock %}
